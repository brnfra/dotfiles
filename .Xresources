Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

URxvt*transparent: true
URxvt.shading:      25
XTerm*renderFont: true
XTerm*faceName: CodeNewRoman NF 
rxvt*faceName:  CodeNewRoman NF 

rxvt-unicode*faceName:  CodeNewRoman NF 

XTerm*faceSize: 12
XTerm*utf8: 2
XTerm*locale: true

URxvt.cursorBlink:True
URxvt.pointerBlank:True
URxvt.pointerBlankDelay:1
URxvt.cursorUnderline:True

! Every shell is a login shell by default (for inclusion of all necessary environment variables)
XTerm*loginshell: true
rxvt-unicode*loginshell: true

!! Font prefferenes
URxvt*imLocale: pt_BR.UTF-8
URxvt*termName: rxvt-unicode-256color
URxvt*buffered: false
URxvt.intensityStyles: false
URxvt.font:xft:UbuntuMono Nerd:size=10, \
          :xft:Inconsolata Nerd Font Mono:size=10, \
          :xft:DejaVuSansMono Nerd Font:size=10, \
          :xft:DroidSansMono Nerd Font:size=10, \
          :xft:FuraMono Nerd Font:size=10, \
          :xft:RobotoMono Nerd Font:pixelsize=15, \
          :xft:Font Awesome 5 Brand Regular:pixelsize=15, \
          :xft:Noto Sans Mono:size=10, \
          :xft:Monospace:style=Medium:size=10   
urxvt.font: xft:CodeNewRoman NF:size=13
rxvt.font: xft:CodeNewRoman NF:size=13
rxvt-unicode.font: xft:CodeNewRoman NF:size=13

URxvt.saveLines: 200000
URxvt.scrollBar: false
URxvt.cursorColor: white
URxvt*scrollWithBuffer: true
URxvt*scrollTtyOutput:true
!
!! Yeah, I am one of those, who use these keys in Vim :-(
urxvt.keysym.Home:          \033[1~
urxvt.keysym.End:           \033[4~
urxvt.keysym.Control-Up:    \033[1;5A
urxvt.keysym.Control-Down:  \033[1;5B
urxvt.keysym.Control-Left:  \033[1;5D
urxvt.keysym.Control-Right: \033[1;5C
!! Copy Paste & Other Extensions
URxvt.perl-ext-common: default,matcher,clipboard-osc,selection,selection-to-clipboard,selection-pastebin,selection-popup,confirm-paste,resize-font
!paste Shift+insert
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard

URxvt.clipboard.autocopy: true
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
URxvt.resize-font.step: 2
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1

! Colorscheme

!! URxvt Appearance
URxvt.letterSpace: -1
URxvt.lineSpace:  -1
Rxvt.lineSpace:  -1
URxvt.geometry: 92x24
URxvt.internalBorder: 14

! special
URxvt*depth: 32
*.foreground: #daffc2
*.background: #222200
*.cursorColor: #feffcf

! black
*.color0: #052a4e
*.color8: #000000

! red
*.color1: #d12f2c
*.color9: #fa3935

! green
*.color2: #559f00
*.color10: #83f047

! yellow
*.color3: #999747
*.color11: #dede58

! blue
*.color4: #2587cc
*.color12: #2ca2f5

! magenta
*.color5: #696ebf
*.color13: #8086e8

! cyan
*.color6: #289c93
*.color14: #33c5ba

! white
*.color7: #fff6e0
*.color15: #ffffff

