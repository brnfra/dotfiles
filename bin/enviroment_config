# shellcheck source=~/bin/dotfiles_env
. dotfiles_env
for file in ~/bin/{dotfiles_env,dirs_config,git_config,ssh_config,get_fonts}; do
	[ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;

clnInfo "Installing Enviroment."
make_dirs
git_env
ssh_env
get_fonts
i3_config

