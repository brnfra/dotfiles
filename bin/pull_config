#           causados pelo uso deste script.
for file in ~/bin/{bare_repo_pull,local_repo_pull,ssh_config}; do
    [ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;
ssh_connect
bare_repo_pull
local_repo_pull
