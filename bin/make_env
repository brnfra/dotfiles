#           causados pelo uso deste script.
for file in ~/bin/{dotfiles_env,make_dirs,get_fonts,git_env,ssh_env}; do
    [ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;

clnInfo "Installing Enviroment."
make_dirs
config_git
config_ssh
get_fonts
i3_config_install

